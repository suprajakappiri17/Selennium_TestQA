package nn;

public class Test1 {

	public static void main(String[] args) {
		String s = "java,selenium,POM,cucumber";
		String[] arr = s.split(",");
		for (int i = 0; i < arr.length; i++) {
			System.out.println(arr[i]);
		}
		String s1="Supraja";
		for(int i=s1.length()-1;i>0;i--)
		{
			System.out.println(s1.charAt(i));
			
		}

	}

}
